{
  "name": "server",
  "version": "1.0.0",
  "description": "This is the backend of the project",
  "main": "src/server.js",
  "scripts": {
    "build:webpack-dev": "webpack --mode development --config webpack.config.js --progress --color",
    "build:webpack-prod": "webpack --mode production --config webpack.config.js",
    "start:webpack": "node build/bundle.js",
    "build:babel": "babel src --out-dir dist --copy-files --ignore node_modules,build --source-maps inline",
    "start:babel": "node dist/server.js",
    "dev": "nodemon",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "mongoose",
    "jwt",
    "bcrypt",
    "cors",
    "dotenv",
    "nodemon",
    "eslint",
    "prettier"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^2.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
